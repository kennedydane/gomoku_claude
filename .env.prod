# Production environment - secure configuration
POSTGRES_DB=gomoku_prod_db
POSTGRES_USER=gomoku_user  
POSTGRES_PASSWORD=your_very_secure_production_password_change_this
POSTGRES_PORT=  # Don't expose PostgreSQL port in production

# pgAdmin disabled in production
PGADMIN_DEFAULT_EMAIL=admin@gomoku.com
PGADMIN_DEFAULT_PASSWORD=your_secure_pgadmin_password_change_this
PGADMIN_PORT=  # Don't expose pgAdmin in production

# Backend API
DEBUG=false
SECRET_KEY=your-super-secure-production-secret-key-use-openssl-rand-hex-32
API_PORT=8000

# Production profiles
COMPOSE_PROFILES=production